[
  {
    "WorkflowName": "ExcludeUL",
    "Rules": [
      {
        "Enabled": true,
        "RuleName": "UlTextEmpty",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "text.Length == 0 OR childcount == 0",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "UlBlockQuoteEmbedded",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "parenttagname == \"blockquote\" OR selector.Contains(\">blockquote\")",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "UlLiEmbedded",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "parenttagname == \"li\" OR selector.Contains(\">li\")",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "UlListTextContains",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "text.Contains(\"Bookmark\") OR text.Contains(\"Share on\") OR text.Contains(\"Share Article\") OR text.Contains(\"Share This Story\", StringComparison.InvariantCultureIgnoreCase) OR text.Contains(\"Click to Share\", StringComparison.InvariantCultureIgnoreCase)",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "UlShare",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "id == \"post_meta\" OR id.StartsWith(\"sharebar\") OR text.StartsWith(\"Share\")",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "UlClassListContains",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "classlist.Contains(\"rotator-panels\") OR classlist.Contains(\"rotator-pages\") OR classlist.Contains(\"playlist\") OR classlist.Contains(\"article-social\") OR classlist.Contains(\"xwv-rotator\") OR classlist.Contains(\"a-social-share-spacing\") OR classlist.Contains(\"socialShare\") OR classlist.Contains(\"heateor_sssp_sharing_ul\") OR classlist.Contains(\"list-none\") OR classlist.Contains(\"essb_links_list\") OR classlist.Contains(\"simple-list\") OR classlist.Contains(\"td-category\") OR classlist.Contains(\"social-icons__list\") OR classlist.Contains(\"authors\")",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "UlParentClassListContains",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "parentclasslist.Contains(\"sd-content\") OR parentclasslist.Contains(\"editorial\")",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "UlNyPostExclude",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "name == \"new york post\" AND childclasslist.Contains(\"story--list\")",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "UlOannExclude",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "name == \"one america news network\" AND childclasslist.Contains(\"dpsp-network-list-item\")",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "UlWsjExclude",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "name == \"wsj\"",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "UlPoliticoExclude",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "name == \"politico\" AND tagname == \"ol\"",
        "SuccessEvent": "true"
      }
    ]
  },
  {
    "WorkflowName": "ExcludeHeader",
    "Rules": [
      {
        "Enabled": true,
        "RuleName": "HeaderTextEmpty",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "text.Length == 0",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "HeaderBlockQuoteEmbedded",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "parenttagname == \"blockquote\" OR selector.Contains(\">blockquote\")",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "HeaderLiEmbedded",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "parenttagname == \"li\" OR selector.Contains(\">li\")",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "HeaderTextUseless",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "text == \"Advertisement\" OR text == \"Follow Us\" OR text == \"You may like\" OR text == \"Share\" OR text == \"Conversation\" OR text == \"Related\" OR text == \"Related Stories\" OR text == \"Share:\"",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "HeaderNyPostExclude",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "name == \"new york post\" AND (text.StartsWith(\"More On:\") OR text.StartsWith(\"see also\"))",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "HeaderWsjExclude",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "name == \"wsj\" AND text.StartsWith(\"What's News\")",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "HeaderPoliticoExclude",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "name == \"politico\" AND (text.StartsWith(\"About The Author\") OR text.StartsWith(\"Most Read\"))",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "HeaderYahooSportsExclude",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "name == \"sports.yahoo.com\" AND tagname == \"h2\"",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "HeaderYahooExclude",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "name == \"www.yahoo.com\" AND text == \"Trending\"",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "HeaderRevolverNewsExclude",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "name == \"revolver news\" AND text == \"Experience Revolver without ads\"",
        "SuccessEvent": "true"
      }
    ]
  },
  {
    "WorkflowName": "ExcludeParagraph",
    "Rules": [
      {
        "Enabled": true,
        "RuleName": "ParagraphTextEmpty",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "tagname != \"figure\" AND text.Length == 0",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "ParagraphBlockQuoteEmbedded",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "parenttagname == \"blockquote\" OR selector.Contains(\">blockquote\")",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "ParagraphLiEmbedded",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "tagname != \"figure\" AND (parenttagname == \"li\" OR selector.Contains(\">li\"))",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "ParagraphFigCaptionEmbedded",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "parenttagname == \"figcaption\" OR selector.Contains(\">figcaption\")",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "ParagraphTextUseless",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "text == \"Advertisement\" OR text == \"Follow Us\" OR text == \"Sign In\"",
        "SuccessEvent": "true"
      },
      {
        "Enabled": true,
        "RuleName": "ParagraphRevolverNewsExclude",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "name == \"revolver news\" AND text == \"Hide ads now\"",
        "SuccessEvent": "true"
      }
    ]
  }
]